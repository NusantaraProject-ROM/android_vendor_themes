<fonts-modification version="1">
    <family customizationType="new-named-family" name="google-sans">
        <font weight="400" style="normal">GoogleSans-Regular.ttf</font>
        <font weight="400" style="italic">GoogleSans-Italic.ttf</font>
        <font weight="500" style="normal">GoogleSans-Medium.ttf</font>
        <font weight="500" style="italic">GoogleSans-MediumItalic.ttf</font>
        <font weight="700" style="normal">GoogleSans-Bold.ttf</font>
        <font weight="700" style="italic">GoogleSans-BoldItalic.ttf</font>
    </family>
    <alias name="google-sans-medium" to="google-sans" weight="500" />

    <family customizationType="new-named-family" name="circularstd">
      <font weight="400" style="normal">CircularStd.ttf</font>
    </family>

    <family customizationType="new-named-family" name="Slate">
        <font weight="100" style="normal">SlateForOnePlus-Thin.ttf</font>
        <font weight="100" style="italic">Roboto-ThinItalic.ttf</font>
        <font weight="200" style="normal">SlateForOnePlus-Light.ttf</font>
        <font weight="200" style="italic">Roboto-LightItalic.ttf</font>
        <font weight="300" style="normal">SlateForOnePlus-Book.ttf</font>
        <font weight="300" style="italic">Roboto-LightItalic.ttf</font>
        <font weight="400" style="normal">SlateForOnePlus-Book.ttf</font>
        <font weight="400" style="italic">Roboto-Italic.ttf</font>
        <font weight="500" style="normal">SlateForOnePlus-Regular.ttf</font>
        <font weight="500" style="italic">Roboto-MediumItalic.ttf</font>
        <font weight="600" style="normal">SlateForOnePlus-Medium.ttf</font>
        <font weight="600" style="italic">Roboto-MediumItalic.ttf</font>
        <font weight="700" style="normal">SlateForOnePlus-Medium.ttf</font>
        <font weight="700" style="italic">Roboto-BoldItalic.ttf</font>
        <font weight="800" style="normal">SlateForOnePlus-Bold.ttf</font>
        <font weight="800" style="italic">Roboto-BoldItalic.ttf</font>
        <font weight="900" style="normal">SlateForOnePlus-Black.ttf</font>
        <font weight="900" style="italic">Roboto-BlackItalic.ttf</font>
    </family>
    <alias name="Slate" to="Slate" weight="500"/>
    <alias name="Slate-thin" to="Slate" weight="100" />
    <alias name="Slate-light" to="Slate" weight="200" />
    <alias name="Slate-book" to="Slate" weight="300" />
    <alias name="Slate-medium" to="Slate" weight="500" />
    <alias name="Slate-bold" to="Slate" weight="700" />
    <alias name="Slate-black" to="Slate" weight="900" />

    <family customizationType="new-named-family" name="san-francisco-pro-display">
      <font weight="400" style="normal">SFProDisplay-Regular.ttf</font>
      <font weight="500" style="normal">SFProDisplay-Medium.ttf</font>
    </family>
    <alias name="san-francisco-pro-display-medium" to="san-francisco-pro-display" weight="500" />

    <family customizationType="new-named-family" name="san-francisco-pro-text">
      <font weight="400" style="normal">SFProText-Regular.ttf</font>
      <font weight="500" style="normal">SFProText-Medium.ttf</font>
    </family>
    <alias name="san-francisco-pro-text-medium" to="san-francisco-pro-text" weight="500" />

    <family customizationType="new-named-family" name="aclonica">
      <font weight="300" style="normal">Aclonica.ttf</font>
    </family>
    <alias name="aclonica-bold" to="aclonica" weight="600" />
    <alias name="times" to="aclonica" />
    <alias name="times new roman" to="aclonica" />
    <alias name="palatino" to="aclonica" />
    <alias name="georgia" to="aclonica" />
    <alias name="baskerville" to="aclonica" />
    <alias name="goudy" to="aclonica" />
    <alias name="fantasy" to="aclonica" />
    <alias name="ITC Stone Serif" to="aclonica" />

